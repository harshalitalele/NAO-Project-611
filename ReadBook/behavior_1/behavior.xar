              <script language="4">
                <content>
                  <![CDATA[from urllib2 import urlopen
import os.path
from os import path
import os
import sys

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        #put initialization code here
        self.memory = self.session().service("ALMemory")
        self.tts = self.session().service('ALTextToSpeech')
        self.ttsStop = self.session().service('ALTextToSpeech') #Create another service as wait is blocking if audioout is remote
        self.bIsRunning = False
        self.ids = []


    def onUnload(self):
        #put clean-up code here
        for id in self.ids:
            try:
                self.ttsStop.stop(id)
            except:
                pass
        while( self.bIsRunning ):
            time.sleep( 0.2 )

    def onInput_inputP(self, number):
        self.logger.info(number)


    def onInput_onStart(self, p):
        self.bIsRunning = True
        last_page_read=1
        #os.remove("last_page_read.txt")
        if path.exists("last_page_read.txt"):
            with open('last_page_read.txt', 'r') as the_file:
                last_page_read=int(the_file.readline())
        #last_page_read=14
        if path.exists("test2.txt"):
            f = open('test.txt', 'r')
            result = f.read()
            f.close()
        else:
            page = urlopen('https://raw.githubusercontent.com/harshalitalele/BookReading/master/An%20Idea.txt')
            page.readline()
            page.readline()

            line="1"
            a_dict={}
            book_page=""
            a_dict[book_page]=[]

            while line:
              line = page.readline().decode('utf-8')
              if "page" in line:
                book_page = line.replace("-","").rstrip()
                a_dict[book_page]=[]
              else:
                a_dict[book_page].append(line.rstrip())

#            f = open('test.txt', 'w')
#            f.write(result)
#            f.close()


        sentence = ''
        try:
#            sentence = "\RSPD="+ str( 100 ) + "\ "
#            sentence += "\VCT="+ str( 100 ) + "\ "

#            sentence += "".join(a_dict["page "+str(last_page_read)])
            #sentence += str(last_page_read)

            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.tts.pCall("say",str(sentence))
            self.ids.append(id)
            self.tts.wait(id)

        finally:
            try:
                self.ids.remove(id)
                last_page_read += 1
                if len(a_dict) == last_page_read:
                    self.tts.pCall("say",str("We have finished the book!"))
                    last_page_read=1
                with open('last_page_read.txt', 'w') as the_file:
                    the_file.write(str(last_page_read))
            except:
                pass
            if( self.ids == [] ):
                self.onStopped() # activate output of the box
                self.bIsRunning = False


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
